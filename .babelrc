{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ]
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    ["import", {
      "libraryName": "antd",
      "style": "css" // `style: true` 会加载 less 文件
    }]
  ]
}